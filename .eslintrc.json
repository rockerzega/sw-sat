{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["standard", "prettier"],
    "globals": {
      "__DEV__": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
      "no-multi-str": "off",
      "no-redeclare": "off",
      "@typescript-eslint/no-redeclare": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": ["error"]
    }
  }